{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/v1/auth/(.*)", "dest": "./auth.router.js" },
    { "src": "/api/v1/user/(.*)", "dest": "./user.router.js" },
    { "src": "/api/v1/chatbot/(.*)", "dest": "./chatbot.router.js" },
    { "src": "/product/(.*)", "dest": "./product.route.js" },
    { "src": "/(.*)", "dest": "./app.js" }
  ]
}
