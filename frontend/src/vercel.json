{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*.+(js|jsx|json|css)",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "../../backend/index.js"
      }
    ]
  }
  